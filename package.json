{
  "name": "bush",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run dev:api & bun run dev:web",
    "dev:api": "bun --watch --env-file .env.local src/api/index.ts",
    "dev:web": "bash -c 'set -a; source .env.local; set +a; PORT=3000 exec next dev src/web'",
    "dev:worker": "bun --watch --env-file .env.local src/media/worker.ts",
    "worker": "bun --env-file .env.local src/media/worker.ts",
    "build": "next build src/web",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "bun --env-file .env.local src/db/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:seed": "bun --env-file .env.local src/db/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.990.0",
    "@aws-sdk/s3-request-presigner": "^3.990.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@workos-inc/authkit-nextjs": "^2.14.0",
    "@workos-inc/node": "^8.5.0",
    "bullmq": "^5.69.3",
    "drizzle-orm": "^0.29.0",
    "hono": "^4.0.0",
    "ioredis": "^5.3.0",
    "next": "^15.5.12",
    "pdfjs-dist": "^5.4.624",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "better-sqlite3": "^11.0.0",
    "bun-types": "^1.2.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "drizzle-kit": "^0.20.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^2.0.0"
  }
}
