{
  "name": "bush",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:api & npm run dev:web",
    "dev:api": "tsx watch --env-file=.env.local src/api/index.ts",
    "dev:web": "next dev src/web",
    "build": "next build src/web",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx --env-file=.env.local src/db/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:seed": "tsx --env-file=.env.local src/db/seed.ts"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "@hono/node-server": "^1.8.0",
    "ioredis": "^5.3.0",
    "drizzle-orm": "^0.29.0",
    "better-sqlite3": "^11.0.0",
    "zod": "^3.22.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0",
    "drizzle-kit": "^0.20.0",
    "eslint": "^9.0.0"
  }
}
